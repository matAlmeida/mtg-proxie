{
  "name": "mtg-proxy",
  "version": "0.2.0",
  "description": "Create MTG clean Proxie Cards using a MOL list as entry.",
  "keywords": [
    "magic",
    "mtg",
    "magic the gathering",
    "proxie",
    "card"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/matAlmeida/mtg-proxy.git"
  },
  "private": false,
  "bin": {
    "mtg-proxy": "bin/mtg-proxy"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,json} && standard --fix",
    "lint": "standard",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
