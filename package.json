    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "gluegun": "^2.1.0",
    "html5-to-pdf": "^3.1.3",
    "jimp": "^0.6.4"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "prettier": "^1.12.1",
    "standard": "^12.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
